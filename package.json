{
  "name": "google-maps-js-api-react",
  "version": "1.2.3",
  "description": "Library for convenient work of Google Maps JavaScript API with React",
  "author": "Krombik",
  "license": "MIT",
  "repository": "Krombik/google-maps-react",
  "main": "dist/index.js",
  "module": "dist/index.modern.js",
  "source": "src/index.tsx",
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "build": "microbundle --jsx React.createElement",
    "start": "microbundle --jsx React.createElement watch",
    "prepare": "run-s build",
    "predeploy": "cd example && yarn install && yarn run build",
    "deploy": "gh-pages -d example/build"
  },
  "peerDependencies": {
    "react": ">=16.8.0",
    "react-dom": ">=16.8.0"
  },
  "dependencies": {
    "google-maps-js-api-loader": "^2.0.1",
    "marker-cluster": "^1.2.1",
    "fast-deep-equal": "^3.1.3"
  },
  "devDependencies": {
    "@types/google.maps": "^3.49.2",
    "@types/react": "^18.0.12",
    "@types/react-dom": "^18.0.5",
    "cross-env": "^7.0.3",
    "gh-pages": "^4.0.0",
    "microbundle": "^0.15.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.7.0",
    "typescript": "^4.7.3"
  },
  "files": [
    "dist"
  ],
  "keywords": [
    "react",
    "google",
    "maps",
    "loader",
    "clustering",
    "Marker",
    "Map",
    "OverlayView",
    "AutocompleteService",
    "autocomplete"
  ]
}
