{
  "name": "google-maps-js-api-react",
  "version": "1.1.0",
  "description": "Library for convenient work of Google Maps JavaScript API with React",
  "author": "Krombik",
  "license": "MIT",
  "repository": "Krombik/google-maps-react",
  "main": "dist/index.js",
  "module": "dist/index.modern.js",
  "source": "src/index.tsx",
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "build": "microbundle --jsx React.createElement",
    "start": "microbundle --jsx React.createElement watch",
    "prepare": "run-s build",
    "predeploy": "cd example && yarn install && yarn run build",
    "deploy": "gh-pages -d example/build"
  },
  "peerDependencies": {
    "react": ">=16.8.0",
    "react-dom": ">=16.8.0"
  },
  "dependencies": {
    "google-maps-js-api-loader": "^1.1.2",
    "marker-cluster": "^1.2.0",
    "fast-deep-equal": "^3.1.3"
  },
  "devDependencies": {
    "@types/google.maps": "^3.47.3",
    "@types/react": "^17.0.38",
    "@types/react-dom": "^17.0.11",
    "@typescript-eslint/eslint-plugin": "^5.10.0",
    "@typescript-eslint/parser": "^5.10.0",
    "babel-eslint": "^10.1.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.7.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-config-standard": "^16.0.3",
    "eslint-config-standard-react": "^11.0.1",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-promise": "^6.0.0",
    "eslint-plugin-react": "^7.28.0",
    "eslint-plugin-standard": "^5.0.0",
    "gh-pages": "^3.2.3",
    "microbundle": "^0.14.2",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.5.1",
    "typescript": "^4.5.5"
  },
  "files": [
    "dist"
  ],
  "resolutions": {
    "**/typescript": "^4.5.5"
  },
  "keywords": [
    "react",
    "google",
    "maps",
    "loader",
    "clustering",
    "Marker",
    "Map",
    "OverlayView",
    "AutocompleteService",
    "autocomplete"
  ]
}
