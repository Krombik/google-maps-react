{
  "name": "google-maps-react",
  "version": "1.0.0",
  "description": "Made with create-react-library",
  "author": "Krombik",
  "license": "MIT",
  "repository": "Krombik/google-maps-react",
  "main": "dist/index.js",
  "module": "dist/index.modern.js",
  "source": "src/index.tsx",
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "build": "microbundle-crl --no-compress --css-modules false --format modern,cjs",
    "start": "microbundle-crl watch --no-compress --format modern,cjs",
    "prepare": "run-s build",
    "test": "run-s test:unit test:lint test:build",
    "test:build": "run-s build",
    "test:lint": "eslint .",
    "test:unit": "cross-env CI=1 react-scripts test --env=jsdom",
    "test:watch": "react-scripts test --env=jsdom",
    "predeploy": "cd example && yarn install && yarn run build",
    "deploy": "gh-pages -d example/build"
  },
  "peerDependencies": {
    "react": ">=16.8.0",
    "react-dom": ">=16.8.0"
  },
  "dependencies": {
    "@googlemaps/js-api-loader": "^1.12.3",
    "@googlemaps/markerclusterer": "^1.0.8",
    "kdbush": "^3.0.0",
    "lodash.throttle": "^4.1.1",
    "marker-cluster": "link:../marker-cluster"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^12.1.0",
    "@testing-library/user-event": "^13.2.1",
    "@types/google.maps": "^3.45.6",
    "@types/jest": "^25.1.4",
    "@types/kdbush": "^3.0.0",
    "@types/lodash.throttle": "^4.1.6",
    "@types/node": "^12.12.38",
    "@types/react": "^17.0.21",
    "@types/react-dom": "^17.0.9",
    "@typescript-eslint/eslint-plugin": "^4.31.1",
    "@typescript-eslint/parser": "^4.31.1",
    "babel-eslint": "^10.1.0",
    "cross-env": "^7.0.3",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-config-standard": "^16.0.3",
    "eslint-config-standard-react": "^11.0.1",
    "eslint-plugin-import": "^2.24.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-promise": "^5.1.0",
    "eslint-plugin-react": "^7.25.2",
    "eslint-plugin-standard": "^5.0.0",
    "gh-pages": "^3.2.3",
    "microbundle-crl": "^0.13.11",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.4.1",
    "react": ">=16.8.0",
    "react-dom": ">=16.8.0",
    "react-scripts": "^5.0.0",
    "typescript": "^4.5.4"
  },
  "files": [
    "dist"
  ],
  "resolutions": {
    "**/typescript": "^4.5.4"
  }
}
