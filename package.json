{
  "name": "google-maps-js-api-react",
  "version": "3.0.0",
  "description": "Library for convenient work of Google Maps JavaScript API with React",
  "author": "Krombik",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Krombik/google-maps-react.git"
  },
  "bugs": {
    "url": "https://github.com/Krombik/google-maps-react/issues"
  },
  "homepage": "https://github.com/Krombik/google-maps-react#readme",
  "type": "module",
  "source": "src/index.ts",
  "exports": {
    "require": "./dist/index.cjs",
    "default": "./dist/index.modern.js"
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.module.js",
  "types": "./dist/index.d.ts",
  "unpkg": "./dist/index.umd.js",
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "build": "microbundle --jsx React.createElement",
    "start": "microbundle --jsx React.createElement watch"
  },
  "peerDependencies": {
    "react": ">=16.8.0",
    "react-dom": ">=16.8.0"
  },
  "peerDependenciesMeta": {
    "@types/react": {
      "optional": true
    }
  },
  "dependencies": {
    "google-maps-js-api-loader": "^2.2.1"
  },
  "devDependencies": {
    "@swc/core": "^1.3.37",
    "@types/google.maps": "^3.52.1",
    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.0.11",
    "cross-env": "^7.0.3",
    "typedoc": "0.23.26",
    "microbundle": "^0.15.1",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.8.4",
    "tsup": "^6.6.3",
    "typescript": "^4.9.5"
  },
  "files": [
    "dist",
    "src"
  ],
  "keywords": [
    "react",
    "google",
    "maps",
    "loader",
    "Marker",
    "Map",
    "OverlayView",
    "AutocompleteService",
    "autocomplete",
    "Polygon",
    "Polyline",
    "Circle",
    "Rectangle",
    "DrawingManager",
    "HeatmapLayer",
    "GoogleMap"
  ],
  "sideEffects": false
}
